-- 수학 문제별 득점자수, 득점률
select 
(select count(*) from scoring where subjectID=3 and a01=1) as 득점자수, 
(select count(*) from scoring where subjectID=3 and a01=1)/count(*) * 100 as 득점률 
from scoring where subjectID=3; # 수학 a01 득점자수, 득점률
